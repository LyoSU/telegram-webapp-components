{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "outDir": "./dist",
    "target": "es2019",
    "lib": [
      "es2019"
    ],
    "declaration": true,
    "strict": false,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true
  },
  "include": [
    "src/"
  ]
}
